{
    "name": "thewebmen/silverstripe-elemental-grid",
    "description": "Elemental grid module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks",
        "grid"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0",
        "dnadesign/silverstripe-elemental": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "elemental-grid",
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "resources"
        ]
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\ElementalGrid\\": "src/"
        }
    }
}
